<%= form_for user, url: admins_users_path, method: :post do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :uname, 'User name' %>
      <%= f.text_field :uname, class: 'form-control' %>
    </div>
    <div>
      <%= f.hidden_field :business_id %>
    </div>
    <br>
    <div>
      <%= f.submit class: 'btn btn-primary' %>
    </div>
<% end %>